{
    "name": "kduma/eloquent-snowflakeable",
    "description": "Eases using and generating SnowFlake ID's in Laravel Eloquent models.",
    "keywords": [
        "laravel",
        "eloquent",
        "snowflake"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Krystian Duma",
            "email": "git@krystian.duma.sh"
        }
    ],
    "require": {
        "php": ">=8.1",
        "godruoyi/php-snowflake": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "KDuma\\Eloquent\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "KDuma\\Eloquent\\SnowflakeableServiceProvider"
            ]
        }
    }
}
